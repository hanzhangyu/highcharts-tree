{
  "name": "highcharts-tree",
  "version": "0.1.0",
  "description": "Create tree type in Highcharts",
  "author": "paulhan <zhangyu.han@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanzhangyu/highcharts-tree"
  },
  "homepage": "https://github.com/hanzhangyu/highcharts-tree",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "tsc --watch & parcel example/index.html",
    "build": "tsc",
    "lint": "tslint --project \"./tsconfig.json\"",
    "report": "nyc report --reporter=html",
    "ut": "tsc && nyc ava --verbose --color",
    "test": "npm run lint && npm run ut",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "highcharts",
    "tree",
    "dendrogram"
  ],
  "peerDependencies": {
    "highcharts": "^5.0.0"
  },
  "devDependencies": {
    "@types/highcharts": "^5.0.38",
    "ava": "^1.3.1",
    "coveralls": "^3.0.3",
    "nyc": "^13.3.0",
    "parcel-bundler": "^1.12.2",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333"
  },
  "files": [
    "lib",
    "src",
    "types"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "test/*.test.ts"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "src/index.ts"
    ]
  },
  "dependencies": {
    "highcharts": "^5.0.0"
  }
}
